@Article{Matthiessen2015,
  author   = {Matthiessen, Christian MIM},
  title    = {Register in the round: registerial cartography},
  journal  = {Functional Linguistics},
  year     = {2015},
  pages    = {1--48},
  volume   = {2},
  number   = {1},
  abstract = {Registerial cartography is the activity of systematically describing the registers that make up a language --- with register in its original sense of a functional variety of a language, i.e. of the adaptation of the meaning-making resources of a language according to context of use. A register map of a language would thus show its composition of registers --- of meanings at risk in the various cultural domains that constitute a culture. This variation according to use --- register variation --- is located along the cline of instantiation between the overall meaning potential of a language operating in the context of culture and the instantiation of this meaning potential unfolding as texts in contexts of situation.},
