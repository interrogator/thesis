@Article{lange_publicly_2007,
  author       = {Lange, Patricia G.},
  title        = {Publicly private and privately public: {{Social}} networking on {{YouTube}}},
  pages        = {361--380},
  volume       = {13},
  number       = {1},
  abstract     = {YouTube is a public video-sharing website where people can experience varying degrees of engagement with videos, ranging from casual viewing to sharing videos in order to maintain social relationships. Based on a one-year ethnographic project, this article analyzes how YouTube participants developed and maintained social networks by manipulating physical and interpretive access to their videos. The analysis reveals how circulating and sharing videos reflects different social relationships among youth. It also identifies varying degrees of 'publicness' in video sharing. Some participants exhibited 'publicly private' behavior, in which video makers' identities were revealed, but content was relatively private because it was not widely accessed. In contrast, 'privately public' behavior involved sharing widely accessible content with many viewers, while limiting access to detailed information about video producers' identities. (PsycINFO Database Record (c) 2008 APA, all rights reserved) (from the journal abstract)},
  date         = {2007},
  file         = {j.1083-6101.2007.00400.x (1).pdf:/Users/daniel/Library/Application Support/Firefox/Profiles/jad6urms.default/zotero/storage/7CVPT2PZ/j.1083-6101.2007.00400.x (1).pdf:application/pdf},
  journaltitle = {Journal of Computer-Mediated Communication},
  keywords     = {Digital Video,Interpersonal Relationships,Online Social Networks,privately public behavior,publicly private behavior,Sharing (Social Behavior),Social Behavior,Social Networks,social relationships,video sharing,YouTube},
  timestamp    = {2016-06-01T15:07:00Z},
  url          = {plange@cinema.usc.eduhttp://ezproxy.lib.vt.edu:8080/login?url=http://search.ebscohost.com/login.aspx?direct=true&db=psyh&AN=2007-19531-018&site=ehost-live&scope=site},
}

